{
    "manifest_version": 2,
    "name": "UV - Beta",
    "version": "0.0.1",
    "description": "hide anything you don't want to see on any webpage with just a shift-click",
    "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "author": "basiclaser",

    "content_scripts": [
        {
            "matches": ["*://*./*"],
            "css": ["uv.css"],
            "js": ["js/jquery.js","js/jquery.hotkeys.js", "js/uv.js"]
    }
  ]

}
